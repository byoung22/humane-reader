{
   "manifest_version": 3,
   "name": "Humane Reader",
   "version": "1.0",
   "description": "Generating natural sounding speach from text using AI.",
   "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
   },
   "permissions": [
      "activeTab",
      "scripting"
   ],
   "host_permissions": [
      "http://localhost:5000/"
   ],
   "action": {
      "default_popup": "popup.html"
   }
}